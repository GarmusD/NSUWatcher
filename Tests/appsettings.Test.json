{
  "NsuSys": {
    "bossac": {
      "cmd": "/usr/bin/bossac-udoo",
      "port": "ttymxc3"
    },
    "mcuPauseBoot": false,
    "cmdsToExec": [
      "relay open 17"
    ]
  },
  "transport": {
    "serial": {
      "comPort": "/dev/ttymxc3",
      "baudRate": 115200,
      "rebootMcu": {
        "baudRate": 9600,
        "dtrPulseOnly": false,
        "delay": 200,
        "reconnectDelay": 1000
      }
    }
  },
  "netServer": {
    "port": 4444
  },
  "Serilog": {
    "MinimumLevel": "Information"
  },
  "TelegramLog": {
    "minimumLevel": "warning",
    "botToken": "",
    "channelId": 0
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
